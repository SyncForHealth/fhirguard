    {
      "code": "AMENDER",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Amender",
      "description": "A person who has corrected, edited, or amended pre-existing information.",
      "filter": null
    },
    {
      "code": "COAUTH",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Co-Author",
      "description": "The entity that co-authored content. There can be multiple co-authors of content, which may take such forms as a contract, a healthcare record entry or document, a policy, or a consent directive.",
      "filter": null
    },
    {
      "code": "CONT",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Contact",
      "description": "A person or an organization that provides or receives information regarding another entity. Examples; patient NOK and emergency contacts; guarantor contact; employer contact.",
      "filter": null
    },
    {
      "code": "EVTWIT",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Event Witness",
      "description": "A person who attests to observing an occurrence.  For example, the witness has observed a procedure and is attesting to this fact.",
      "filter": null
    },
    {
      "code": "PRIMAUTH",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Primary Author",
      "description": "An entity that is the primary or sole author of information content.  In the healthcare context, there can be only one primary author of health information content in a record entry or document.",
      "filter": null
    },
    {
      "code": "REVIEWER",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Reviewer",
      "description": "A person, device, or algorithm that has used approved criteria for filtered data for inclusion into the patient record.  Examples: (1) a medical records clerk who scans a document for inclusion in the medical record, enters header information, or catalogues and classifies the data, or a combination thereof; (2) a gateway that receives data from another computer system and interprets that data or changes its format, or both, before entering it into the patient record.",
      "filter": null
    },
    {
      "code": "SOURCE",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Source",
      "description": "An automated data source that generates a signature along with content. Examples: (1) the signature for an image that is generated by a device for inclusion in the patient record; (2) the signature for an ECG derived by an ECG system for inclusion in the patient record; (3) the data from a biomedical monitoring device or system that is for inclusion in the patient record.",
      "filter": null
    },
    {
      "code": "TRANS",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Transcriber",
      "description": "An entity entering the data into the originating system. This includes the transcriptionist for dictated text transcribed into electronic form.",
      "filter": null
    },
    {
      "code": "VALID",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Validator",
      "description": "A person who validates a health information document for inclusion in the patient record. For example, a medical student or resident is credentialed to perform history or physical examinations and to write progress notes. The attending physician signs the history and physical examination to validate the entry for inclusion in the patient's medical record.",
      "filter": null
    },
    {
      "code": "VERF",
      "type": [
        "concept"
      ],
      "system": "http://terminology.hl7.org/CodeSystem/contractsignertypecodes",
      "display": "Verifier",
      "description": "A person who asserts the correctness and appropriateness of an act or the recording of the act, and is accountable for the assertion that the act or the recording of the act complies with jurisdictional or organizational policy. For example, a physician is required to countersign a verbal order that has previously been recorded in the medical record by a registered nurse who has carried out the verbal order.",
      "filter": null
    },
